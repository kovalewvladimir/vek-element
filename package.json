{
  "name": "element-plus-aa-workspaces",
  "type": "module",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "npm run dev -w=element-plus-aa-example",
    "preview": "npm run preview -w=element-plus-aa-example",
    "publish": "npm publish -w=element-plus-aa",
    "lint": "eslint .",
    "type-check": "vue-tsc --noEmit -p ./apps/example/tsconfig.json",
    "check": "npm run lint && npm run type-check",
    "ncu": "ncu -ws --dep prod,peer,dev",
    "ncu:u": "ncu -ws --dep prod,peer,dev -u",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/node": "22.7.9",
    "@unocss/eslint-config": "0.63.6",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/eslint-config-typescript": "14.1.3",
    "eslint": "9.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "husky": "9.1.6",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "unocss": "0.63.6",
    "unplugin-element-plus": "0.8.0",
    "vite": "5.4.10",
    "vite-svg-loader": "5.1.0",
    "vue-tsc": "2.1.10"
  }
}
